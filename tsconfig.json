/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "fullTemplateTypeCheck": true,
    "preserveWhitespaces": false,
    "skipLibCheck": true,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true,
    "trace": true,
  },
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "lib": [
      "es2020",
      "dom"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "outDir": "./dist/out-tsc",
    "paths": {
      "@angular-package/*": [
        "./node_modules/@angular-package/*"
      ],
      "@angular/*": [
        "./node_modules/@angular/*"
      ],
      "calculation": [
        "dist/calculation"
      ],
      "callback": [
        "dist/callback"
      ],
      "change-detection": [
        "dist/change-detection"
      ],
      "component-loader": [
        "dist/component-loader"
      ],
      "console": [
        "dist/console"
      ],
      "core": [
        "dist/core"
      ],
      "crypto": [
        "node_modules/crypto-js"
      ],
      "currency": [
        "dist/currency"
      ],
      "dart-sass": [
        "dist/dart-sass"
      ],
      "detection": [
        "dist/detection"
      ],
      "error": [
        "dist/error"
      ],
      "font-awesome": [
        "dist/font-awesome"
      ],
      "indexeddb": [
        "dist/indexeddb"
      ],
      "name": [
        "dist/name"
      ],
      "nav": [
        "dist/nav"
      ],
      "payment": [
        "dist/payment"
      ],
      "preferences": [
        "dist/preferences"
      ],
      "prism": [
        "dist/prism"
      ],
      "property": [
        "dist/property"
      ],
      "range": [
        "dist/range"
      ],
      "reactive": [
        "dist/reactive"
      ],
      "sass": [
        "dist/sass"
      ],
      "sass-list": [
        "dist/sass-list"
      ],
      "sass-query": [
        "dist/sass-query"
      ],
      "sass-string": [
        "dist/sass-string"
      ],
      "spectre": [
        "dist/spectre"
      ],
      "spectre.css": [
        "dist/spectre.css"
      ],
      "spectre.dev": [
        "dist/spectre.dev"
      ],
      "state": [
        "dist/state"
      ],
      "storage": [
        "dist/storage"
      ],
      "tag": [
        "dist/tag"
      ],
      "testing": [
        "dist/testing"
      ],
      "text": [
        "dist/text"
      ],
      "type": [
        "dist/type"
      ],
      "ui": [
        "dist/ui"
      ],
      "wrapper": [
        "dist/wrapper"
      ]
    },
    "removeComments": true,
    "sourceMap": true,
    "strict": true,
    "target": "es2020",
    "types": ["node"]
  }
}
