  "todohighlight.defaultStyle": {
      "color": "#000",
      "backgroundColor": "#fff308",
      "overviewRulerColor": "#ffab00",
      "cursor": "pointer",
      "border": "1px solid #eee",
      "borderRadius": "2px",
      "isWholeLine": false
